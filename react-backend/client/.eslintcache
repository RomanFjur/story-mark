",[],["111","112"],"/Users/romanzminko/story-mark/react-backend/client/src/Store/reducer.js",[],"/Users/romanzminko/story-mark/react-backend/client/src/store/reducer.js",[],"/Users/romanzminko/story-mark/react-backend/client/src/services/api.js",[],"/Users/romanzminko/story-mark/react-backend/client/src/containers/Register-form/Register-form.js",["113"],"// Import Libraries\nimport React from 'react';\nimport {Formik, Field, Form, ErrorMessage} from 'formik';\nimport {connect} from 'react-redux';\nimport {withRouter} from \"react-router-dom\";\nimport * as Yup from 'yup';\nimport {registration} from '../../store/actions'\n\n// Import Components\nimport FormTitle from '../../components/Form-title/Form-title';\nimport Button from '../../components/Button/Button';\n\n// Import styles\nimport styles from './Register-form.module.css';\n\n// Create Container\nclass RegisterForm extends React.Component {\n  render() {\n    return (\n      <Formik\n        initialValues = {{\n          name: '',\n          email: '',\n          password: '',\n          repeatPassword: '',\n          checkbox: false\n        }}\n        validationSchema = {Yup.object({\n          name: Yup.string()\n            .max(15, 'Must be 15 characters or less')\n            .required('Required'),\n          email: Yup.string()\n            .email('Incorrect value. Should be email')\n            .required('Required'),\n          password: Yup.string()\n            .max(20, 'Must be 20 characters or less')\n            .required('Required'),\n          repeatPassword: Yup.string()\n            .max(20, 'Must be 20 characters or less')\n            .oneOf([Yup.ref('password'), null], 'Passwords must match')\n            .required('Repeat password is required'),\n        }).shape({\n          checkbox: Yup.bool().oneOf([true], 'Accept Privacy Policy is required')\n        })}\n        onSubmit = {(values, {setSubmitting}) => {\n          setSubmitting(false);\n          this.props.registration(values);\n          setTimeout(() => {\n            this.props.history.push('/users');\n          }, 100);\n        }}\n      > \n        {formik => {\n          return (\n            <Form noValidate className={styles.form}>\n              <FormTitle \n                title=\"One more step to make history\"\n                desc=\"Mark uses our app for more than 20 times per day! Be like Mark\"\n                titleStyle=\"formsTitle\"\n                descStyle=\"descTitle\"\n                styling=\"registerFormTitle\"\n              />\n              <div className={styles.formBlock}>\n                <label htmlFor=\"name\" className={styles.label}>Name</label>\n                <Field name=\"name\" type=\"text\" className={styles.input} />\n                <ErrorMessage name=\"name\"></ErrorMessage>\n                <label htmlFor=\"email\" className={styles.label}>Email</label>\n                <Field name=\"email\" type=\"email\" className={styles.input} />\n                <ErrorMessage name=\"email\"></ErrorMessage>\n                <label htmlFor=\"password\" className={styles.label}>Password</label>\n                <Field name=\"password\" type=\"password\" className={styles.input} />\n                <ErrorMessage name=\"password\"></ErrorMessage>\n                <label htmlFor=\"repeatPassword\" className={styles.label}>Repeat password</label>\n                <Field name=\"repeatPassword\" type=\"password\" className={styles.input} />\n                <ErrorMessage name=\"repeatPassword\"></ErrorMessage>\n                <div className={styles.privacyBlock}>\n                  <Field name=\"checkbox\" type=\"checkbox\" className={styles.privacyCheckbox} />\n                  <label htmlFor=\"checkbox\" className={styles.desc}>I agree with our Privacy Policy</label>\n                  <ErrorMessage name=\"checkbox\" component=\"div\"/>\n                </div>\n                <Button type=\"submit\" styling=\"submit\" disabled={!(formik.dirty && formik.isValid)} >Sign In</Button>\n                <Button type=\"reset\" styling=\"reset\">Cancel</Button>\n              </div>\n            </Form>\n          )\n        }} \n      </Formik>\n    );\n  }\n}\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    registration: (values) => {\n      dispatch({type: \"ADD_USER\", payload: values});\n    }\n  }\n}\n\nexport default withRouter(connect(null, mapDispatchToProps)(RegisterForm));","/Users/romanzminko/story-mark/react-backend/client/src/containers/Login-form/Login-form.js",[],"/Users/romanzminko/story-mark/react-backend/client/src/components/Form-title/Form-title.js",[],"/Users/romanzminko/story-mark/react-backend/client/src/components/Button/Button.js",[],"/Users/romanzminko/story-mark/react-backend/client/src/store/actions.js",["114","115","116","117","118","119","120","121","122","123","124","125","126","127","128"],"// export const registration = (user) => {\n//   return async (dispatch, getState) => {\n//     dispatch(getTokenStarted());\n//     try {\n//       const createdToken = await endpointCreateUser(user);\n//       localStorage.setItem(\"token\", createdToken.token);\n//       dispatch(getTokenSuccess(createdToken.token));\n//       const success = await endpointLoginAsUser(getState());\n//       dispatch(loginUserSuccess(success));\n//       return;\n//     } catch (error) {\n//       dispatch(failure(error.message));\n//       return;\n//     }\n//   };\n// };\n\n// export const findUser = (user) => {\n//   return async (dispatch, getState) => {\n//     dispatch(getTokenStarted());\n//     try {\n//       const success = await endpointLoginAsUser(user);\n//       localStorage.setItem(\"token\", success.token);\n//       dispatch(getTokenSuccess(success.token));\n//       const auth = await endpointLoginAsUser(getState());\n//       dispatch(loginUserSuccess(auth));\n//       const users = await endpointLoadUsers();\n//       dispatch(getUsersSuccess(users));\n//       console.log(success, auth, us