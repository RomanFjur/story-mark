{
  "name": "redux-when",
  "version": "2.1.3",
  "description": "Redux middleware for delaying dispatch of an action until a condition evaluates to true.",
  "keywords": [
    "redux",
    "wait",
    "when",
    "until",
    "for",
    "delay",
    "once"
  ],
  "main": "./dist/index.js",
  "types": "./index.d.ts",
  "repository": "jameslnewell/redux-when",
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "redux": "^3.3.1",
    "redux-mock-store": "^1.2.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  },
  "scripts": {
    "build": "babel ./index.js -d ./dist/",
    "watch": "npm run build -- --watch",
    "test": "mocha --compilers js:babel-register",
    "example": "babel-node ./example/index.js",
    "prepublish": "npm run build"
  }
}
